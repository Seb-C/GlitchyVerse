		<translate group="door_1_2" start="0" end="1500" from="0, 0.9, 0"     to="0, 0, 0" />
		<translate group="door_2_2" start="0" end="1500" from="-0.9, 0.9, 0"  to="0, 0, 0" />
		<translate group="door_3_2" start="0" end="1500" from="0.9, 0.9, 0"   to="0, 0, 0" />
