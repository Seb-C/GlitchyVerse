		<translate group="door_3_4" start="1000" end="2500" from="0, 0, 0" to="0.9, 0, 0" />
	</animation>
	<animation id="close">
		<rotate group="lock" start="0" end="0" origin="0, 0, 0" from="0, 0, 0" to="0, 0, 270"  />
		<rotate group="lock" start="1500" end="2500" origin="0, 0, 0" from="0, 0, 0" to="0, 0, -270" />
		<translate group="lock_front" start="0" end="1500" from="-0.9, 0.9, 0" to="0, 0, 0" />
		<translate group="lock_back"  start="0" end="1500" from="0.9, -0.9, 0" to="0, 0, 0" />
