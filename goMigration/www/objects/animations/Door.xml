<?xml version="1.0" encoding="utf-8" ?>
<animated object="Door">
	<animation id="open">
		<rotate group="lock" start="0" end="1000" origin="0, 0, 0" from="0, 0, 0" to="0, 0, 270" />
		<translate group="lock_front" start="1000" end="2500" from="0, 0, 0" to="-0.9, 0.9, 0" />
		<translate group="lock_back"  start="1000" end="2500" from="0, 0, 0" to="0.9, -0.9, 0" />
		<translate group="door_2_1" start="1000" end="2500" from="0, 0, 0" to="-0.9, 0, 0" />
		<translate group="door_3_1" start="1000" end="2500" from="0, 0, 0" to="0.9, 0, 0" />
		<translate group="door_1_2" start="1000" end="2500" from="0, 0, 0" to="0, 0.9, 0" />
		<translate group="door_2_2" start="1000" end="2500" from="0, 0, 0" to="-0.9, 0.9, 0" />
		<translate group="door_3_2" start="1000" end="2500" from="0, 0, 0" to="0.9, 0.9, 0" />
		<translate group="door_4_2" start="1000" end="2500" from="0, 0, 0" to="0, 0.9, 0" />
