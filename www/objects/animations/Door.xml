<?xml version="1.0" encoding="utf-8" ?>
<animated object="Door">
	<animation id="open">
		<rotate    group="lock"      start="0"    end="1000" origin="0, -0.075, 0" from="0, 0, 0" to="0, 0, 270" />
		<translate group="doorLeft"  start="1000" end="2500"                       from="0, 0, 0" to="-0.9, 0, 0" />
		<translate group="doorRight" start="1000" end="2500"                       from="0, 0, 0" to="0.9, 0, 0" />
	</animation>
	<animation id="close">
		<rotate    group="lock"      start="0"    end="0"    origin="0, -0.075, 0" from="0, 0, 0"    to="0, 0, 270" />
		<translate group="doorLeft"  start="0"    end="1500"                       from="-0.9, 0, 0" to="0, 0, 0" />
		<translate group="doorRight" start="0"    end="1500"                       from="0.9, 0, 0"  to="0, 0, 0" />
		<rotate    group="lock"      start="1500" end="2500" origin="0, -0.075, 0" from="0, 0, 0"    to="0, 0, -270" />
	</animation>
</animated>